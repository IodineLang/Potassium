class RequestParameters {
    func RequestParameters(self, queryString) {
        self.parameters = self.parse(queryString);
    }
    
    func parse(self, queryString) {
        if (queryString == null) {
            return hashMap();
        }
        pairs = queryString.split("&");
        if (pairs[0] == "") {
            return hashMap();
        }
        query_dict = hashMap();
        foreach (pair in pairs) {
            pair_items = pair.split("=");
            query_dict[pair_items[0]] = pair_items[1];
        }
        return query_dict;
    }
    
    func getParam(self, key) {
        if (self.parameters.contains(key)) {
            return self.parameters[key];
        }
        else {
            return null;
        }
    }

}

class Request {
    func Request(parameters) {
        self.parameters = parameters;
    }
}
